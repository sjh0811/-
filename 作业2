
void setup() {

size(600, 500);
background(255);

}

void draw(){
fill(0);
line(0,0,600,0);
line(0,0,0,500);
line(0,500,600,500);
line(600,0,600,500);
line(100,0,100,300);
line(0,300,200,300);
line(0,50,250,50);
line(0,200,250,200);
line(250,0,250,500);
line(500,0,500,400);
line(250,100,500,100);
line(450,0,450,100);
line(250,200,400,200);
line(400,100,400,250);
line(250,250,500,250);
line(500,200,600,200);
line(0,350,250,350);
line(200,200,200,500);
line(50,300,50,500);
line(50,450,600,450);
line(250,400,600,400);
line(450,400,450,500);
line(350,250,350,400);
strokeWeight(5);
fill(102,255,204);//green
rect(100,50,150,150);
rect(0,300,50,50);
rect(250,200,150,50);
rect(500,0,100,200);
rect(50,350,150,100);
fill(204,0,51);//red
rect(250,0,200,100);
rect(50,300,150,50);
rect(250,250,100,150);
rect(500,200,100,200);
rect(450,450,150,50);
rect(400,100,100,150);
fill(255,193,37);//yellow
rect(0,0,100,50);
rect(0,200,100,100);
rect(200,200,50,150);
rect(250,400,200,50);
rect(0,350,50,150);
rect(200,450,50,50);
fill(204,153,255);//purple
rect(0,50,100,200);
fill(0,102,204);//blue
rect(100,0,100,50);
rect(100,200,100,100);
rect(250,100,150,100);
rect(450,0,50,100);
}
